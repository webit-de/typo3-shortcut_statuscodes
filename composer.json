{
  "name": "webit-de/shortcut_statuscodes",
  "description": "Different HTTP statuscodes for shortcuts",
  "homepage": "https://github.com/webit-de/typo3-shortcut_statuscodes",
  "license": "GPL-2.0-or-later",
  "type": "typo3-cms-extension",
  "require": {
    "php": ">=7.2.0",
    "typo3/cms-core": "^9.5 || ^10.4"
  },
  "autoload": {
    "psr-4": {
      "WebitDe\\ShortcutStatuscodes\\": "Classes/"
    }
  },
  "replace": {
    "typo3-ter/shortcut_statuscodes": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "shortcut_statuscodes"
    }
  }
}
